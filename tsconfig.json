{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
     "jsx": "react-native",
    //"jsx": "react-jsx",
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx","src/**/*.js", "src/**/*.tsx", "src/**/*.jsx"],
  // "exclude": ["node_modules"]
}